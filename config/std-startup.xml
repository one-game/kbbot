<aiml version="1.0.1" encoding="UTF-8">
    <!-- std-startup.xml -->
 
    <!-- Category是一个自动的AIML单元 -->
    <category>
 
        <!-- Pattern用来匹配用户输入 -->
        <!-- 如果用户输入 "LOAD AIML B" -->
        <pattern>LOAD AIML B</pattern>
 
        <!-- Template是模式的响应 -->
        <!-- 这里学习一个aiml文件 -->
        <template>
            <learn>basic_chat.aiml</learn>
			<learn>basic_chat_cn.aiml</learn>
			<learn>other.aiml</learn>
            <!-- 你可以在这里添加更多的aiml文件 -->
            <!--<learn>more_aiml.aiml</learn>-->
        </template>
 
    </category>
 
</aiml>